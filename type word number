#include<iostream>
#include<string>

using namespace std;

int main ()
{
    //interduse the values
    int a;
    int part1;
    int part2;

    string str1;
    string str2;

    cout<<"Type number between 10 and 99 please : ";
    cin>>a;

    part1 = a / 10;
    part2 = a % 10;    
    //program make erorr when number is wrong
    if (a < 10 || a > 99 )
    {
        cout<<"!!!!!Please enter valid number!!!!! \n";
    }
    //while numbers in valid 
    //we want print the word of that number 
    else if( a < 20 )
    {
        
        switch(a)
        {

        case 10:
            cout<<"Ten \n";
            break;

        case 11:
            cout<<"eleven \n";
            break;

        case 12:
            cout<<"twelve \n";
            break;

        case 13:
            cout<<"thirteen \n";
            break;

        case 14:
            cout<<"fourteen \n";
            break;

        case 15:
            cout<<"fifteen \n";
            break;

        case 16:
            cout<<"sixteen \n";
            break;

        case 17:
            cout<<"seventeen \n";
            break;

        case 18:
            cout<<"eighteen \n";
            break;

        case 19:
            cout<<"nineteen \n";
            break;

        default:
            break;
        }
        return 0;
    }
    else
    {
        switch (part1)
        {

        case 2:
            str1 = "twenty";
            break;

        case 3:
            str1 = "therty";
            break;

        case 4:
            str1 = "forthy";
            break;

        case 5:
            str1 = "fifty";
            break;

        case 6:
            str1 = "sixty";
            break;

        case 7:
            str1 = "seventy";
            break;

        case 8:
            str1 = "eighty";
            break;

        case 9:
            str1 = "niney";
            break;

        default:
            break;
        }    
        switch (part2)
        {

        case 0:
            str2 = "";
            break;

        case 1:
            str2 = "one";
            break;

        case 2:
            str2 = "two";
            break;

        case 3:
            str2 = "three";
            break;

        case 4:
            str2 = "four";
            break;

        case 5:
            str2 = "five";
            break;

        case 6:
            str2 = "six";
            break;

        case 7:
            str2 = "seven";
            break;

        case 8:
            str2 = "eight";
            break;

        case 9:
            str2 = "nine";
            break;

        default:
            break;
        }    
    }

   cout<<str1<<str2<<endl;
   return 0;
   //thanks to see
}                                                                                                
